    <mapping resource="lotto/model/AdminInfo.hbm.xml"/>
    <mapping resource="lotto/model/StudentGroup.hbm.xml"/>
    <mapping resource="lotto/model/StudentInfo.hbm.xml"/>
    <mapping resource="lotto/model/VenueSection.hbm.xml"/>
    <mapping resource="lotto/model/VenueRow.hbm.xml"/>
    <mapping resource="lotto/model/LotteryEvent.hbm.xml"/>
    <mapping resource="lotto/model/LotterySeat.hbm.xml"/>
    <mapping resource="lotto/model/LotteryRegistration.hbm.xml"/>
    <mapping resource="lotto/model/VenueSeat.hbm.xml"/>
    <mapping resource="lotto/model/VenueInfo.hbm.xml"/>
    <mapping resource="lotto/model/MailingAddress.hbm.xml"/>
